<div class="container jumbotron text-center">
  <h3>Sherpa Sign-Up</h3>
  <h2>Please Enter your information to Signup For a Sherpa Account</h2>
</div>

<div class="container jumbotron text-center center-block">
  <%= form_for @user do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control'%><br>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control'%><br>

      <%= f.submit 'Sign Up', class: 'btn btn-primary' %>

  <% end %>
</div>


</div>
